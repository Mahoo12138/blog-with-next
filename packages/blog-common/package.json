{
  "name": "@blog/common",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": "true",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "types": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx"
      ],
      "development": "./src/*"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf .turbo",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@blog/data": "workspace:*",
    "@blog/design-system": "workspace:*",
    "@blog/utils": "workspace:*",
    "@reduxjs/toolkit": "^2.2.7",
    "@vercel/examples-ui": "^1.0.5",
    "clsx": "^1.2.1",
    "next-themes": "^0.3.0",
    "react-content-loader": "^7.0.2",
    "react-hotkeys-hook": "^3.4.7",
    "react-redux": "^9.1.2",
    "redux-saga": "^1.3.0",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.10",
    "eslint": "^8.38.0",
    "eslint-config-blog": "workspace:*",
    "typescript": "^5.0.4"
  }
}